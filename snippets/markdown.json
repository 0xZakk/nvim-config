{
  "Changelog": {
    "prefix": "change",
    "body": [
      "#### $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE - $1",
      "",
      "- $2"
    ]
  },
  "Zettel link": {
    "prefix": "zl",
    "body": [
      "[[$1]]"
    ]
  },
  "Internal Link": {
    "prefix": "internal",
    "body": [
      "{: .internal-link}"
    ]
  },
  "Domain": {
    "prefix": "domain",
    "body": [
      "---",
      "title: \"$1\"",
      "layout: note",
      "type: domain",
      "---",
      "",
      "## Metadata",
      "",
      "**Type:** [[Areas of Interest]]"
    ]
  },
  "Note": {
    "prefix": "note",
    "body": [
      "---",
      "title: \"$1\"",
      "type: note",
      "published: true",
      "date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND",
      "---",
      "$5",
      "## Metadata",
      "\n",
      "**Source:** $2",
      "\n",
      "**Relevant Context(s):**",
      "- $3",
      "\n",
      "**Related Notes:**",
      "- $4"
    ]
  },
  "Reference": {
    "prefix": "ref",
    "body": [
      "---",
      "title: \"$1\"",
      "type: reference",
      "published: true",
      "date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND",
      "---",
      "$5",
      "## Metadata",
      "\n",
      "**Source:** $2",
      "\n",
      "**Author: $3**",
      "\n",
      "**Relevant Context(s):**",
      "- $4"
    ]
  },
  "Post": {
    "prefix": "post",
    "body": [
      "---",
      "title: \"$1\"",
      "type: post",
      "layout: post",
      "published: true",
      "date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
      "tags: $2",
      "excerpt: $3",
      "---",
      "$4"
    ]
  }
}

